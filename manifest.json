{
   "author": "martin",
   "background": {
       "scripts": ["headers.js"]
   },
   "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "script.js" ],
      "matches": [ "*://*/*" ],
      "match_about_blank": true,
      "run_at": "document_start"
   } ],
   "description": "Defuses the dumbest attempts at detecting devtools",
   "icons": {
      "128": "icon128.png",
      "32": "icon32.png"
   },
   "browser_action": {
       "default_icon": "icon.png"
   },
   "manifest_version": 2,
   "name": "anti anti devtools",
   "short_name": "AAD",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.3"
}
